{
  "name": "passment",
  "version": "0.0.1",
  "private": true,
  "dependencies": {
    "@loadable/component": "^5.12.0",
    "firebase": "^7.6.2",
    "normalize.css": "^8.0.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^5.2.1",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.3.0",
    "react-snapshot": "^1.3.0"
  },
  "scripts": {
    "dev": "react-scripts start",
    "build": "react-scripts build",
    "production": "GENERATE_SOURCEMAP=false react-scripts build && npm run postbuild",
    "postbuild": "react-snapshot",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "deploy": "npm run production && firebase deploy",
    "serve": "npm run production && firebase serve --only hosting",
    "serve-test": "npm run build && firebase serve --only hosting",
    "analyze": "source-map-explorer 'build/static/js/**.js'"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "main": "index.js",
  "devDependencies": {
    "source-map-explorer": "^2.2.2"
  },
